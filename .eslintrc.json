{
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": ["prettier", "airbnb-base"],
    "plugins": [
        "prettier",
        "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "prettier/prettier": "error",
      "max-len": ["error", {"code": 80, "ignoreUrls": true}],
      "allowElseIf": true

    }
  }
